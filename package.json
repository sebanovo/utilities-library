{
  "name": "utilities-library",
  "version": "3.2.0",
  "main": "./dist/index.js",
  "bin": "./dist/index.js",
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "build": "tsc",
    "lint": "eslint --ext .ts,.js . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --ext .ts,.js . --fix",
    "docs": "jsdoc -c jsdoc.json -R README.md",
    "prepare": "npm run build && npm run docs"
  },
  "keywords": [
    "tensor",
    "vector",
    "string",
    "number"
  ],
  "engines": {
    "node": ">=17.0.1"
  },
  "engine-strict": true,
  "author": "Sebastian Cespedes Rodas",
  "license": "ISC",
  "description": "tensor - matrix - vector - string - number",
  "devDependencies": {
    "@pixi/jsdoc-template": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "jsdoc": "^4.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebanovo/utilities-library.git"
  },
  "homepage": "https://utilities-library.vercel.app/"
}
