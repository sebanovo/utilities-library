{
  "name": "utilities-library",
  "version": "3.2.9",
  "files": [
    "dist"
  ],
  "type": "commonjs",
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "jsdelivr": "./dist/index.min.js",
  "unpkg": "./dist/index.min.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "ts-node ./src/index.ts",
    "build": "rollup -c",
    "lint": "eslint --ext .ts,.js . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --ext .ts,.js . --fix",
    "jsdoc": "jsdoc -c jsdoc.json -R README.md",
    "tsdoc": "typedoc --tsconfig ./tsconfig.json",
    "prepare": "npm run build && npm run tsdoc"
  },
  "keywords": [
    "datastructure",
    "circular linked list",
    "double linked list",
    "linked list",
    "tensor",
    "vector",
    "string",
    "number",
    "stack",
    "queue"
  ],
  "engines": {
    "node": ">=17.0.1"
  },
  "engine-strict": true,
  "author": "Sebastian Cespedes Rodas",
  "license": "ISC",
  "description": "data structure library",
  "devDependencies": {
    "@pixi/jsdoc-template": "^2.6.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.25.2",
    "rollup": "^4.28.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "varvara-typedoc-theme": "^0.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebanovo/utilities-library.git"
  },
  "homepage": "https://utilities-library.vercel.app/",
  "dependencies": {
    "jsdoc": "^4.0.4",
    "typedoc": "^0.27.5"
  }
}
