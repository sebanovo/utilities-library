{
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": ["./dist/"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/docs)"
  },
  "opts": {
    "recurse": "true",
    "destination": "./doc",
    "template": "./node_modules/@pixi/jsdoc-template"
  },
  "templates": {
    "applicationName": "Utilities Library",
    "favicon": "./public/perfil.png",
    "disqus": "",
    "googleAnalytics": "",
    "default": {
      "staticFiles": {
        "include": ["./public/"]
      }
    },
    "openGraph": {
      "title": "Utilities Library Docs",
      "type": "website",
      "image": "/perfil.png",
      "site_name": "utilities-library",
      "url": "https://utilities-library"
    },
    "meta": {
      "title": "Utilities Library",
      "description": "library to manage tensors, vectors, matrices, numbers and strings",
      "keyword": "Library"
    },
    "linenums": true,
    "source": {
      "include": ["./dist/"],
      "includePattern": ".+\\.js(doc)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "encoding": "utf8",
      "recurse": true,
      "private": false,
      "lenient": true,
      "destination": "./docs",
      "template": "./node_modules/@pixi/jsdoc-template"
    }
  },
  "pixi": {
    "styles": [],
    "scripts": []
  }
}
